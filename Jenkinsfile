pipeline {
    agent any

    stages {
        stage('Initialize') {
            steps {
                echo 'Initializing..'
            }
        }
        stage('Build') {
            steps {
                echo 'Building..'
            }
        }
        stage('content-release') {
            steps {
                echo 'Deploying contents....'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying code....'
            }
        }
    }
}
